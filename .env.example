# Environment Variables Template
# Copy this file to .env and set your actual values
# NOTE: .env will be ignored by git - never commit actual secrets!

# GitHub API Configuration
GITHUB_TOKEN=your_github_personal_access_token_here

# AWS Configuration (for S3 operations)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-west-2

# Optional: Override default configuration
CONFIG_FILE=config.yaml
STATE_FILE=release_state.json

# Development settings
DEBUG=false
LOG_LEVEL=INFO

ARTIFACTORY_URL="http://localhost:8081/artifactory"
ARTIFACTORY_REPOSITORY="generic-releases"
ARTIFACTORY_API_KEY="your-generated-api-key"